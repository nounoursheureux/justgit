html
  body
    h1 Yolo
    h2 /#{dir}
    ul
      each item in files
        if item.isTree()
          li 
            em: a(href='/' + repo + '/tree/'+branch+'/'+dir+'/'+item.filename()) #{item.filename()}
        else if item.isBlob()
          li
            a(href='/' + repo + '/tree/'+branch+'/'+dir+'/'+item.filename()) #{item.filename()}
